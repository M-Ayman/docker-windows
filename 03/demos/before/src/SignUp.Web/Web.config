      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date [%-2thread] %-5level - %message%newline" />
      </layout>
    </appender>
    <logger name="SignUp.Log">
      <level value="DEBUG" />
      <appender-ref ref="RollingLogFileAppender" />
    </logger>
  </log4net>
  
